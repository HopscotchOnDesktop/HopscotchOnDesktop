<Window x:Class="HS.PluginAuthor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HS"
        mc:Ignorable="d"
        Title="PluginAuthor" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="codeBox" TextChanged="LoadSuggestions" TextWrapping="Wrap" Text="TextBox" Margin="10,33,10,10"/>
        <TextBox x:Name="UUID" TextWrapping="Wrap" Height="18" Text="" Width="100" Margin="50,10,0,0" VerticalAlignment="Top" HorizontalAlignment="Left"/>
        <Label Content="UUID" Padding="0" Width="30" Height="18" Margin="10,10,0,0" VerticalAlignment="Top" HorizontalAlignment="Left"/>
        <Button Click="Test_Plugin" Content=">>" Width="30" Height="18" Padding="0" Margin="155,10,0,0" VerticalAlignment="Top" HorizontalAlignment="Left"/>

    </Grid>
</Window>
